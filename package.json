    "dependencies": {
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "backblaze-b2": "^1.7.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "helmet": "^7.1.0",
        "express-rate-limit": "^7.1.5"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "pm2": "^5.3.0"
    }
}
